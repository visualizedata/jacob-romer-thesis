{
  "manifest_version": 2,

  "name": "Trackers",
  "description": "",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content_style.css", "animate.css"],
      "js": ["jquery.js", "typed.js", "pt.min.js", "content_script.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["eventPage.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}
